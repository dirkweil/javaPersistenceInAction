<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <navigation-rule>
  <from-view-id>/view/personList.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>edit</from-outcome>
   <to-view-id>/view/personEdit.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/view/personEdit.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>ok</from-outcome>
   <to-view-id>/view/personList.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>next</from-outcome>
   <to-view-id>/view/personEdit2.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/view/personEdit2.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>back</from-outcome>
   <to-view-id>/view/personEdit.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>ok</from-outcome>
   <to-view-id>/view/personList.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <locale-config>
   <default-locale>de</default-locale>
   <supported-locale>de</supported-locale>
   <supported-locale>en</supported-locale>
  </locale-config>
 </application>
</faces-config>
